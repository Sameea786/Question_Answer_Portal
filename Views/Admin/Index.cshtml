
@{
    Layout = null;
}<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <style>
        .carousel-inner > .item > img,
        .carousel-inner > .item > a > img {
            width: 50%;
            margin: auto;
        }
    </style>
    <title></title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="background-color :rgb(219, 224, 156)">
 
    <div class="container">
       
        <div>
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner" role="listbox">
                    <div class="item active">
                        <img src="/Content/themes/base/images/u.jpg" alt="Chania">
                    </div>

                    <div class="item">
                        <img src="/Content/themes/base/images/g.png" alt="Chania">
                    </div>

                    <div class="item">
                        <img src="/Content/themes/base/images/sg.jpg" alt="Flower">
                    </div>


                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
            <form name="nam1" action="~/Admin/Index2" method="get" style="text-align:center">
<pre>
<input type="text" placeholder="Email" style=" font-family: Imprint MT Shadow; color: #b416db; font-size: 20px;" name="email" />

<input type="password" placeholder="****" style="font-family: Imprint MT Shadow; color: #b416db;font-size: 20px" name="password" />

<input type="submit" value="Sign In" style="font-family: Imprint MT Shadow; color: #b416db;font-size: 20px" />
</pre>
            </form>


            <div style="font-family: Imprint MT Shadow; color: #bbf137; font-size: 20px; background-color: #000000; border-radius: 15px ">
                <p style="text-align:center">
                    Email: Askme@gmail.com                         Contact No:92 678908
                    <br />All rights reserved by Ask Me Organization
                </p>

            </div>
        </div>
    </div>


   
</body>
</html>
